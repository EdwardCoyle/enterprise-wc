<ids-app-menu id="app-menu" edge="start" type="app-menu">
      <!-- Avatar/Username Area -->
      <img slot="avatar" id="avatar" alt="Picture of Richard Fairbanks" src="/src/assets/images/avatar-placeholder.jpg">
      <ids-text slot="username" font-size="24" font-weight="semi-bold">Richard Fairbanks</ids-text>

      <!-- Header Toolbar -->
      <ids-toolbar slot="header" role="toolbar">
        <ids-toolbar-section align="center-even" type="fluid">
          <ids-button id="header-btn-download" icon="download"><ids-icon icon="download" class="ids-icon"></ids-icon>
            <ids-text audible="">Download</ids-text>
          </ids-button>
          <ids-button id="header-btn-print" icon="print"><ids-icon icon="print" class="ids-icon"></ids-icon>
            <ids-text audible="">Print</ids-text>
          </ids-button>
          <ids-button id="header-btn-purchasing" icon="purchasing"><ids-icon icon="purchasing" class="ids-icon"></ids-icon>
            <ids-text audible="">Purchasing</ids-text>
          </ids-button>
          <ids-button id="header-btn-notification" icon="notification"><ids-icon icon="notification" class="ids-icon"></ids-icon>
            <ids-text audible="">Notification</ids-text>
          </ids-button>
          <ids-button id="header-btn-inventory" icon="inventory"><ids-icon icon="inventory" class="ids-icon"></ids-icon>
            <ids-text audible="">Inventory</ids-text>
          </ids-button>
        </ids-toolbar-section>
      </ids-toolbar>

      <ids-search-field id="search" clearable="true" color-variant="app-menu" label="Application Menu Search" label-state="collapsed" no-margins="" slot="search" placeholder="Type to search"></ids-search-field>

      <!-- Main Content (Accordion Navigation) -->
      <ids-accordion color-variant="app-menu">
        <ids-accordion-panel color-variant="app-menu">
          <ids-accordion-header slot="header" icon="user" expander-type="caret" color-variant="app-menu" aria-expanded="false">
            <ids-text font-size="16">Employee</ids-text>
          </ids-accordion-header>
          <ids-accordion-panel slot="content" color-variant="sub-app-menu">
            <ids-accordion-header slot="header" color-variant="sub-app-menu" aria-expanded="false">
              <ids-text font-size="14">My Profile</ids-text>
            </ids-accordion-header>
          </ids-accordion-panel>
          <ids-accordion-panel slot="content" color-variant="sub-app-menu">
            <ids-accordion-header slot="header" color-variant="sub-app-menu" aria-expanded="false">
              <ids-text font-size="14">Talent Profile</ids-text>
            </ids-accordion-header>
          </ids-accordion-panel>
          <ids-accordion-panel slot="content" color-variant="sub-app-menu">
            <ids-accordion-header slot="header" color-variant="sub-app-menu" aria-expanded="false">
              <ids-text font-size="14">View Compensation</ids-text>
            </ids-accordion-header>
          </ids-accordion-panel>
          <ids-accordion-panel slot="content" color-variant="sub-app-menu">
            <ids-accordion-header slot="header" color-variant="sub-app-menu" aria-expanded="false">
              <ids-text font-size="14">RAVE</ids-text>
